def two_sum(nums, target):
    hashNum = {}
    for i, n in enumerate(nums):
        if target - n in hashNum:
            return hashNum[target-n], i
        hashNum[n] = i


# print(twoSum(nums=[2,1,5,7],target=4))
print(two_sum(nums=[4,1,9,7,5,3,16],target=14))
print(two_sum(nums=[3,2,4], target=6))    